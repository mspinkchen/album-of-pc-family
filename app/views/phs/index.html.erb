<table class="table table-striped table-condensed">
 <% @phs.each do |ph| %>
   <tr>
     <td><%= ph.title %></td>
     <td><%= link_to 'Show', ph_path(ph) %></td>
     <td><%= link_to 'Edit', edit_ph_path(ph) %></td>
     <td><%= link_to 'Destroy', ph_path(ph), method: :delete, data:{ confirm: "Are you sure?" } %></td>
   </tr>
 <% end %>
</table>

<%= link_to 'New Ph', new_ph_path %>